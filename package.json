{
  "name": "file-service",
  "version": "2.1.9",
  "description": "file-service",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18 || 19 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t file-service:$(npm pkg get version | xargs echo) .",
    "docker:run": "docker run -p 3000:80 -d file-service:$(npm pkg get version | xargs echo)",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Ojvar <MOjvar.AmirHossein@Gmail.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^6.1.1",
    "@loopback/core": "^5.1.1",
    "@loopback/cron": "^0.11.1",
    "@loopback/repository": "^6.1.1",
    "@loopback/rest": "^13.1.1",
    "@loopback/rest-explorer": "^6.1.1",
    "@loopback/service-proxy": "^6.1.1",
    "@sentry/node": "^7.60.0",
    "bson": "^4.7.0",
    "dotenv": "^16.0.2",
    "jssha": "^3.3.0",
    "keycloak-connect": "^20.0.2",
    "loopback-connector-mongodb": "^5.2.3",
    "loopback-connector-rest": "^4.0.2",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.3.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.1",
    "@loopback/eslint-config": "^14.0.2",
    "@loopback/testlab": "^6.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.18.38",
    "eslint": "^8.45.0",
    "husky": "^8.0.0",
    "pinst": "^3.0.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.1.6"
  },
  "copyright.owner": "Ojvar <MOjvar.AmirHossein@Gmail.com>",
  "packageManager": "yarn@3.6.3"
}
